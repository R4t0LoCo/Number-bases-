        "# function to convert a unary number to decimal\n",
        "def to_dec(unary_num):\n",
        "    return len(unary_num)\n",
        "\n",
        "# main program\n",
        "unary_num = input(\"Enter a unary number: \")\n",
        "dec_num = to_dec(unary_num)\n",
        "print(\"Decimal equivalent: \" + str(dec_num))"
