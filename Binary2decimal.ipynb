      "source": [
        "def to_dec(bin_num):\n",
        "    dec_num = 0\n",
        "    for i in range(len(bin_num)):\n",
        "        if bin_num[-(i+1)] == '1':\n",
        "            dec_num += 2**i\n",
        "    return dec_num\n",
        "\n",
        "bin_num = input(\"Enter a binary number: \")\n",
        "dec_num = to_dec(bin_num)\n",
        "print(\"Decimal equivalent: \" + str(dec_num))"
      ],
